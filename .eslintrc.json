{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "off",
    "no-case-declarations": "off",
    "no-unused-vars": "off",
    "no-empty": "off"
  }
}
